# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias -- -="cd -"


# Sidecar
alias ans="cd ~/projects/git/sidecar-cfg"
alias srender="cd ~/projects/git/sidecar-render"
alias sdata="cd ~/projects/git/sidecar-data"
alias sweb="cd ~/projects/git/sidecar-web"
alias code="cd ~/projects/git"
